HTTPClient http;
DHT dht(33, DHT11);
IRsend irsend(4);

void setupWifi() {
  delay(10);
  WiFi.mode(WIFI_STA);
  WiFi.begin(ssid, password);
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
}

void setup() {
  Serial.begin(115200);
  irsend.begin();
  dht.begin();
  setupWifi();
  pinMode(23, OUTPUT);
  pinMode(13, OUTPUT);
  pinMode(32, OUTPUT);
}

void led(String kode) {
  if (kode == "m") {
    digitalWrite(23, HIGH);
    digitalWrite(13, LOW);
    digitalWrite(32, LOW);
  } else if (kode == "k") {
    digitalWrite(23, LOW);
    digitalWrite(13, HIGH);
    digitalWrite(32, LOW);
  } else if (kode == "h") {
    digitalWrite(23, LOW);
    digitalWrite(13, LOW);
    digitalWrite(32, HIGH);
  }
}

void get() {
  http.begin(getInfo);
  http.GET();
  String datas = http.getString();
  DynamicJsonDocument doc(1024);
  deserializeJson(doc, datas);
  const char* index1 = doc["index1"];
  const char* index2 = doc["index2"];
  const char* kode = doc["kode"];
  bool isTrue[2] = {false, false};
  int tempIndex[2] = {0, 0};

  for (int i = 0; i < 15; i++) {
    if(String(index1).toInt() == i) {
      isTrue[0] = true;
      tempIndex[0] = String(index1).toInt();
    }
    if(String(index2).toInt() == i) {
      isTrue[1] = true;
      tempIndex[1] = String(index2).toInt();
    }
  }

  if (isTrue[0] && isTrue[1]) {
    i1 = tempIndex[0];
    i2 = tempIndex[1];
  }
  led(String(kode));
  http.end();
}

void post() {
  float t = dht.readTemperature();
  http.begin(postTemperature);
  http.addHeader("Content-Type", "application/json");
  String postTemp = "{\"temp\":\"" + String(t) + "\"}";
  http.POST(postTemp);
  http.end();
}

void loop() {
  get();
  irsend.sendRaw(rawData[i1][i2], 439, 38);
  delay(5000);
  post();
}